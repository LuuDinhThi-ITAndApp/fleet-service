{
  "name": "mqtt-edge-service",
  "version": "1.0.0",
  "description": "MQTT subscriber with Redis cache, TimescaleDB storage, and Socket.IO streaming",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "debug": "node --inspect -r ts-node/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts"
  },
  "keywords": ["mqtt", "redis", "timescaledb", "socketio", "iot"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "mqtt": "^5.3.5",
    "socket.io": "^4.6.1",
    "redis": "^4.6.13",
    "pg": "^8.11.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/pg": "^8.11.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
